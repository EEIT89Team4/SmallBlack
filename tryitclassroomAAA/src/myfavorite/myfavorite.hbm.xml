<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="myfavorite.MyFavoriteVO" table="myfavorite">

		<composite-id>
		<key-many-to-one name="ProductVO" class="com.product.model.ProductVO" lazy="false">
		<column name="product_no"/>
		</key-many-to-one>
		
		<key-many-to-one name="MemberVO" class="com.member.model.MemberVO" lazy="false">
		<column name="member_no"/>
		</key-many-to-one>
		
<!-- 		<key-property name="product_no" column="product_no"/> -->
<!-- 		<key-property name="member_no" column="member_no"/> -->


		</composite-id>

		

	</class>
</hibernate-mapping>