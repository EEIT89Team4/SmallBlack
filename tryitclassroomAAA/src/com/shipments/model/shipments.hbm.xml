<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.shipments.model.ShipmentsVO" table="shipments">

		<id name="shipments_no" column="shipments_no">
			<generator class="identity" />
		</id>

		<property name="adminster_id" column="adminster_id" />
		<property name="order_no" column="order_no" />
		<property name="receipt_name" column="receipt_name" />
		<property name="receipt_address" column="receipt_address" />
		<property name="receipt_phone" column="receipt_phone" />
		<property name="receiptime" column="receiptime" />
	</class>
</hibernate-mapping>

